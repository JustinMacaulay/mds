{
  "presets": [
    ["@babel/preset-env", { "corejs": 3, "useBuiltIns": "usage", "modules": false }],
    ["@babel/preset-react"]
  ],
  "plugins": [
    [
      "@babel/transform-runtime",
      {
        "corejs": 3,
        "regenerator": true
      }
    ],
    ["@babel/plugin-proposal-class-properties"],
    ["react-hot-loader/babel"]
  ]
}
